{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "types": ["cypress"]
  },
  "include": [
    "cypress.config.ts",
    "cypress/**/*.d.ts",
    "**/*.cy.ts",
    "**/*.cy.tsx",
    "cypress/support/*.ts",
    "cypress/fixtures/*.ts",
    "src/components/**/*.tsx",
    "src/data-contracts/**/*.ts",
    "src/services/**/*.ts"
  ]
}
