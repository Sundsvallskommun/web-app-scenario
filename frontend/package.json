{
  "name": "@sk-web-gui/web-app-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "yarn generate:middleware-envs",
    "dev": "cross-env TEST=true next dev",
    "prebuild": "yarn generate:middleware-envs",
    "build": "next build",
    "prebuild:test": "yarn generate:middleware-envs",
    "build:test": "cross-env TEST=true next build",
    "start": "next start",
    "generate:middleware-envs": "node middleware-envs-generator.mjs",
    "generate:contracts": "cross-env NODE_ENV=development npx tsx ./src/swagger-typescript-api.ts",
    "lint": "next lint --no-cache",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "cypress": "cypress open",
    "cypress:headless": "cross-env TEST=true npx cypress run --component && npx cypress run --e2e",
    "jest": "jest --watch",
    "jest:coverage": "jest --coverage",
    "test": "concurrently \"yarn jest\" \"yarn cypress\"",
    "coverage": "yarn nyc report --reporter=text-summary",
    "coverage:merge": "istanbul-merge --out coverage/summary/coverage-final.json coverage/cypress/coverage-final.json coverage/jest/coverage-final.json && nyc report --reporter=lcov -t coverage/summary --report-dir coverage/summary --check-coverage false",
    "test:coverage": "yarn cypress:headless && yarn jest:coverage && yarn coverage:merge",
    "type-check": "tsc --pretty --noEmit"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^15.3.3",
    "@sk-web-gui/ai": "^1.2.1",
    "@sk-web-gui/core": "^3.14.0",
    "@sk-web-gui/react": "^1.5.0",
    "axios": "^1.9.0",
    "dayjs": "^1.11.13",
    "i18next": "^25.2.1",
    "i18next-resources-to-backend": "^1.2.1",
    "lucide-react": "^0.512.0",
    "next": "^15.3.3",
    "next-i18n-router": "^5.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.54.2",
    "react-i18next": "^15.5.2",
    "sass": "^1.89.1",
    "sharp": "^0.34.2",
    "underscore.string": "^3.3.6",
    "yup": "^1.6.1",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@cypress/code-coverage": "^3.14.4",
    "@eslint/js": "^9.28.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@types/underscore.string": "^0.0.41",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "cypress": "^14.4.1",
    "dotenv": "^16.5.0",
    "dotenv-cli": "^8.0.0",
    "envalid": "^8.0.0",
    "eslint": "^9.28.0",
    "eslint-config-next": "^15.3.3",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jest": "^28.12.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "identity-obj-proxy": "^3.0.0",
    "istanbul-merge": "^2.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.4",
    "postcss-import": "^16.1.0",
    "postcss-nested": "^7.0.2",
    "postcss-nesting": "^13.0.1",
    "prettier": "^3.5.3",
    "swagger-typescript-api": "13.0.3",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.3.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1"
  },
  "browserslist": [
    "defaults and fully supports es6-module"
  ]
}
